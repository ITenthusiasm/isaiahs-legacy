{
  "name": "react-cra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "cypress open",
    "test:headless-e2e": "cypress run --headless --browser chrome",
    "eject": "react-scripts eject",
    "lint": "eslint **/*.{ts,tsx}",
    "reset-json-db": "cd .. && npm run reset-json-db",
    "check-types": "tsc --noEmit && cd cypress && tsc --noEmit"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "history": "^4.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.5",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.22",
    "@types/json-schema-faker": "^0.5.1",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "cypress": "^7.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "json-schema-faker": "^0.5.0-rcv.34",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/src/*",
      "<rootDir>/src/App.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 75,
        "lines": 80
      }
    },
    "transform": {
      "\\.tsx?$": [
        "babel-jest",
        {
          "configFile": "./test-utils/.babelrc"
        }
      ]
    }
  }
}
